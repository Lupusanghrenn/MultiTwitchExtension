{
	"manifest_version": 2,
	"name": "Multi Twitch Extension",
	"short_name":"Multi Twitch",
  "background": {
    "scripts": [ "script_back.js" ]
  },
  "browser_action": {
    "default_icon": "icone.png",
    "default_popup": "index.html",
    "default_title": "Multi twitch"
  },
  "description": "Chrome Extension who allow you to easely follow when your streamers goes live.",
  "icons": {
    "128": "icone128.png",
    "64": "icone64.png",
    "32": "icone32.png",
    "16": "icone16.png"
  },
  "options_page": "page_option.html",
  "permissions": [
    "https://api.twitch.tv/kraken/streams/",
    "notifications",
    "tabs"
  ],
  "version": "1.0",
  "version_name":"Premi√®re version",
  "author":"Lupusanghren @Lupusanghrenn"
}
